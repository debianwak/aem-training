<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="For user  that wants to create fairly quickly different types of pages"
    jcr:primaryType="cq:Template"
    jcr:title="Layout Responsive!"
    allowedPaths="[/content/w3(/.*)?]"
    ranking="{Long}3">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="/apps/w3/components/structure/responsive-page">
        <logo
            jcr:primaryType="nt:unstructured"
            sling:resourceType="w3/components/structure/logo"/>
        <title
            jcr:primaryType="nt:unstructured"
            sling:resourceType="/apps/w3/components/training-title"/>
        <topnav
            jcr:primaryType="nt:unstructured"
            sling:resourceType="w3/components/structure/topnav"/>
        <par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/parsys">
            <text
                jcr:primaryType="nt:unstructured"
                sling:resourceType="w3/components/content/text"
                text="&lt;p>This is a content page&lt;/p>"
                textIsRich="true"/>
        </par>
    </jcr:content>
</jcr:root>
